  "devDependencies": {
    "@types/node": "^24.7.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "@vitest/coverage-v8": "3.2.4",
    "tsconfig-paths": "4.2.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  }
